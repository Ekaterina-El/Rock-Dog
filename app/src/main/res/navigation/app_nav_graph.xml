<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  android:id="@+id/app_nav_graph"
  app:startDestination="@id/welcomeFragment">

  <fragment
    android:id="@+id/songsFragment"
    android:name="el.ka.rockdog.view.ui.SongsFragment"
    android:label="SongsFragment" >
    <action
      android:id="@+id/action_songsFragment_to_playlistsFragment"
      app:destination="@id/playlistsFragment" />
    <action
      android:id="@+id/action_songsFragment_to_playerFragment"
      app:destination="@id/playerFragment" />
    <action
      android:id="@+id/action_songsFragment_to_accountFragment"
      app:destination="@id/accountFragment" />
  </fragment>
  <fragment
    android:id="@+id/playlistsFragment"
    android:name="el.ka.rockdog.view.ui.PlaylistsFragment"
    android:label="PlaylistsFragment" />
  <fragment
    android:id="@+id/playerFragment"
    android:name="el.ka.rockdog.view.ui.PlayerFragment"
    android:label="PlayerFragment" />
  <fragment
    android:id="@+id/accountFragment"
    android:name="el.ka.rockdog.view.ui.AccountFragment"
    android:label="AccountFragment" />
  <fragment
    android:id="@+id/welcomeFragment"
    android:name="el.ka.rockdog.view.ui.WelcomeFragment"
    android:label="WelcomeFragment" >
    <action
      android:id="@+id/action_welcomeFragment_to_songsFragment"
      app:destination="@id/songsFragment" />
    <action
      android:id="@+id/action_welcomeFragment_to_singUpFragment"
      app:destination="@id/singUpFragment" />
    <action
      android:id="@+id/action_welcomeFragment_to_logInFragment"
      app:destination="@id/logInFragment" />
  </fragment>
  <fragment
    android:id="@+id/singUpFragment"
    android:name="el.ka.rockdog.view.ui.auth.SingUpFragment"
    android:label="SingUpFragment" >
    <action
      android:id="@+id/action_singUpFragment_to_logInFragment"
      app:destination="@id/logInFragment" />
  </fragment>
  <fragment
    android:id="@+id/logInFragment"
    android:name="el.ka.rockdog.view.ui.auth.LogInFragment"
    android:label="LogInFragment" />
</navigation>